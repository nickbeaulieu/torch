/// Expect: "19\nstr\n1\ndoSomething voided\ndoSomething is a string\n"

func doSomethingVoid() -> void {
  println("doSomething voided");
}

func doSomethingStr() -> int {
  println("doSomething is a string");
  return 9;
}

func main() -> int {
  let a: int = 9;
  let b: int = 10;
  let c: str = "str";
  println("%d", a + b);
  println("%s", c);

  let fake: bool = true;
  println("%d", fake);

  doSomethingVoid();
  doSomethingStr();
  return 0;
}